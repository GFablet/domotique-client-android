<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ironsecurity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".cameras.MosaicCamerasActivity"></activity>
        <activity android:name=".cameras.ScenariosActivity" />
        <activity android:name=".cameras.PilotageCamerasActivity" />
        <activity android:name=".operate_house.RoomActivity" />
        <activity android:name=".operate_house.RoomSelectionActivity" />
        <activity android:name=".operate_house.FloorSelectionActivity" />
        <activity android:name=".ConfigurationActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>